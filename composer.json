        "nmeta",
        "meta",
        "php",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://monstar-lab.com",
    "authors": [
        {
            "name": "Casper Rasmussen",
            "email": "cr@nodes.dk",
            "role": "CTO & Partner"
        },
